{{!-- You can pass in data from the rendering method, --}}
<div class="SearchContainer">
    <div class="SearchFlex">
        <form action="{{reqPath}}Insert" class="getPokemon" method="post">
            <legend>Insert Into Database</legend>
            {{#each insertField}}
            <div class="formCol">
                <input class='form-control' type="text" name="{{id}}" id="{{id}}" placeholder="{{searchText}}">
            </div>
            {{/each}}
            <div class="formCol">
                <button type="submit" class="btn btn-primary">Insert</button>
            </div>
        </form>
    </div>
</div>